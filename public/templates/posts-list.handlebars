<ul class="breadcrumbs">
{{#if category}}
  <li class="breadcrumb-link"><a href="/home" data-navigo>Home</a></li>
  <li class="breadcrumb-separator">>></li>
  <li class="breadcrumb-link"><span>{{category}}</span></li>
{{/if}}
</ul>
{{#each this.posts}}
  <article class="post-container">
    <div class="post-categories-container">
      <h2 class="post-category" class="post-category"><a href="/category/sub/{{subCategory}}/1" class="post-category-link" data-navigo>{{subCategory}}</a></h3>
    </div>
    <div class="post-title-container">
      <h4><a href="/posts/{{id}}" class="post-title" data-navigo>{{title}}</a></h4>
      <p class="post-author">Posted by {{author}}</p>
    </div>
    <div class="post-content-container">
      <p class="post-content">{{content}}</p>
    </div>
  </article>
{{/each}}